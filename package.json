{
  "name": "hackathon17-server",
  "version": "0.0.1",
  "description": "A server side app using Express",
  "main": "index.js",
  "scripts": {
    "build": "babel src -d build --source-maps",
    "serve": "node build/index.js",
    "debug": "node --debug build/index.js",
    "start": "nodemon --watch src -e js --exec \"npm run build && npm run debug\""
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:kantlove/hackathon17-server.git"
  },
  "keywords": [
    "javascript",
    "express",
    "server"
  ],
  "author": "minhthai",
  "license": "MIT",
  "homepage": "https://github.com/kantlove/hackathon17-server#readme",
  "engines": {
    "node": "6.11.x",
    "npm": "3.10.x"
  },
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "body-parser": "^1.17.2",
    "express": "^4.15.4",
    "express-validator": "^3.2.1",
    "mongoose": "^4.11.7",
    "nodemon": "^1.11.0"
  }
}
